A,EAAA,OAdA,qBAcA;AACA,SAAA,OAAA;AACA,GAhBA;AAiBA,EAAA,OAAA,EAAA;AACA,IAAA,KADA,mBACA;AAAA;;AACA,aAAA,UAAA;;AACA,WAAA,SAAA,GAAA,KAAA;AACA,WAAA,KAAA,CAAA,KAAA,GAAA,IAAA,IAAA,GAAA,OAAA,EAAA;AACA,WAAA,IAAA,GAAA,WAAA,CAAA,YAAA;AACA,QAAA,KAAA,CAAA,KAAA,CAAA,IAAA,IAAA,CAAA;AACA,OAFA,EAEA,KAFA,CAAA;AAGA,KARA;AASA,IAAA,OATA,qBASA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,EAAA;AACA,aAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,CAAA;AACA,OAFA,MAEA;AACA,aAAA,IAAA,GAAA,EAAA;AACA;AACA,KAfA;AAgBA,IAAA,GAhBA,iBAgBA;AACA,WAAA,SAAA,GAAA,IAAA;AACA,WAAA,KAAA,CAAA,GAAA,GAAA,IAAA,IAAA,GAAA,OAAA,EAAA;AACA,WAAA,OAAA;AACA,WAAA,IAAA,CAAA,OAAA,CAAA,KAAA,KAAA;AACA,MAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,IAAA,CAAA;;AACA,aAAA,iBAAA,KAAA,KAAA,CAAA,IAAA,GAAA,IAAA;;AACA,MAAA,aAAA,CAAA,KAAA,IAAA,CAAA;AACA,WAAA,KAAA,CAAA,IAAA,GAAA,CAAA;AACA,KAzBA;AA0BA,IAAA,IA1BA,gBA0BA,IA1BA,EA0BA;AACA,aAAA,IAAA,GAAA,KAAA;AACA,KA5BA;AA6BA,IAAA,OA7BA,mBA6BA,IA7BA,EA6BA;AAAA;;AACA,WAAA,IAAA,CAAA,OAAA,CAAA,UAAA,IAAA,EAAA,KAAA,EAAA;AACA,YAAA,IAAA,CAAA,KAAA,KAAA,IAAA,EAAA;AACA,UAAA,MAAA,CAAA,IAAA,CAAA,MAAA,CAAA,KAAA,EAAA,CAAA;AACA;AACA,OAJA;AAKA,MAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,KAAA,IAAA,CAAA;AACA;AApCA,GAjBA;AAuDA,EAAA,QAAA,EAAA;AACA,IAAA,QADA,sBACA;AACA,aAAA,KAAA,KAAA,CAAA,IAAA,GAAA,GAAA,GAAA,GAAA;AACA;AAHA,GAvDA;AA4DA,EAAA,SA5DA,uBA4DA;AACA,IAAA,aAAA,CAAA,KAAA,IAAA,CAAA;AACA,GA9DA;AA+DA,EAAA,UAAA,EAAA;AACA,IAAA,SAAA,EAAA,SADA;AAEA,IAAA,KAAA,EAAA,KAFA;AAGA,IAAA,OAAA,EAAA,OAHA;AAIA,IAAA,MAAA,EAAA;AAJA;AA/DA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <div class=\"sleep\">\r\n      <van-tabs color=\"#FFC300\">\r\n        <van-tab title=\"考拉睡眠\">\r\n          <div class=\"topsleep\">\r\n            <div class=\"start-sleep sleepbox\" v-if=\"sleepShow\">\r\n              <div class=\"icon\">\r\n                <KoalaIcon></KoalaIcon>\r\n              </div>\r\n              <van-button class=\"btn\" color=\"#7232dd\" plain round @click=\"start\">开始睡眠</van-button>\r\n            </div>\r\n            <div class=\"end-sleep sleepbox\" v-if=\"!sleepShow\">\r\n              <div class=\"timebox\">\r\n                <van-circle\r\n                  v-model=\"currentRate\"\r\n                  :rate=\"setValue\"\r\n                  layer-color=\"#ebedf0\"\r\n                  color=\"rgb(16, 174, 181)\"\r\n                  :text=\"'已睡'+sleep.time+'分钟'\"\r\n                />\r\n                <van-button class=\"endbtn\" color=\"#7232dd\" plain round @click=\"end\">结束睡眠</van-button>\r\n              </div>\r\n            </div>\r\n            <div class=\"history\">\r\n              <Title title=\"历史睡眠时长\"></Title>\r\n              <div class=\"list-box\">\r\n                <Useless v-if=\"list.length===0\"></Useless>\r\n\r\n                <van-swipe-cell v-for=\"item in list\" :key=\"item.start\">\r\n                  <van-cell\r\n                    center\r\n                    :title=\"setTime(item.start)+' (入睡)'\"\r\n                    :value=\"Time(item.time)\"\r\n                    :label=\"setTime(item.end)+' (睡醒)'\"\r\n                  />\r\n                  <template #right>\r\n                    <van-button @click=\"delecte(item.start)\" square type=\"danger\" text=\"删除\" />\r\n                  </template>\r\n                </van-swipe-cell>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </van-tab>\r\n        <van-tab title=\"统计图表\">\r\n          <echart :list=\"list\"></echart>\r\n        </van-tab>\r\n      </van-tabs>\r\n    </div>\r\n  </div>\r\n</template>\r\n<script>\r\nimport KoalaIcon from \"../components/KoalaIcon/KoalaIcon\";\r\nimport Title from \"../components/title/title\";\r\nimport Useless from \"../components/Useless/Useless\";\r\nimport echart from \"../components/echart/sleep\";\r\nimport { Toast } from \"vant\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      sleepShow: true,\r\n      currentRate: 15,\r\n      time: null,\r\n      list: [],\r\n      sleep: {\r\n        start: null,\r\n        end: null,\r\n        time: 0,\r\n      },\r\n    };\r\n  },\r\n  created() {\r\n    this.getData();\r\n  },\r\n  methods: {\r\n    start() {\r\n      Toast(\"已进入睡眠状态~\");\r\n      this.sleepShow = false;\r\n      this.sleep.start = new Date().getTime();\r\n      this.time = setInterval(() => {\r\n        this.sleep.time += 1;\r\n      }, 60000);\r\n    },\r\n    getData() {\r\n      if (localStorage.getItem(\"sleep\")) {\r\n        this.list = JSON.parse(localStorage.getItem(\"sleep\"));\r\n      } else {\r\n        this.list = [];\r\n      }\r\n    },\r\n    end() {\r\n      this.sleepShow = true;\r\n      this.sleep.end = new Date().getTime();\r\n      this.getData();\r\n      this.list.unshift(this.sleep);\r\n      localStorage.setItem(\"sleep\", JSON.stringify(this.list));\r\n      Toast(\"已结束睡眠状态，本次睡眠\" + this.sleep.time + \"分~\");\r\n      clearInterval(this.time);\r\n      this.sleep.time = 0;\r\n    },\r\n    Time(time) {\r\n      return time + \" 分钟\";\r\n    },\r\n    delecte(time) {\r\n      this.list.forEach((item, index) => {\r\n        if (item.start === time) {\r\n          this.list.splice(index, 1);\r\n        }\r\n      });\r\n      localStorage.setItem(\"sleep\", JSON.stringify(this.list));\r\n    },\r\n  },\r\n  computed: {\r\n    setValue() {\r\n      return (this.sleep.time / 480) * 100;\r\n    },\r\n  },\r\n  destroyed() {\r\n    clearInterval(this.time);\r\n  },\r\n  components: {\r\n    KoalaIcon,\r\n    Title,\r\n    Useless,\r\n    echart,\r\n  },\r\n};\r\n</script>\r\n<style scoped lang=\"less\">\r\n.topsleep {\r\n  padding: 0.666667rem 0.266667rem;\r\n}\r\n.sleepbox {\r\n  .icon {\r\n    width: 20%;\r\n    position: relative;\r\n    left: 50%;\r\n    transform: translateX(-50%);\r\n  }\r\n  .btn {\r\n    position: relative;\r\n    left: 50%;\r\n    transform: translateX(-50%);\r\n    width: auto;\r\n    margin-top: 0.4rem;\r\n  }\r\n}\r\n.timebox {\r\n  width: 100%;\r\n  display: flex;\r\n  flex-wrap: wrap;\r\n  justify-content: center;\r\n  align-content: center;\r\n  padding: 0.533333rem;\r\n  flex-direction: column;\r\n  .endbtn {\r\n    margin-top: 0.4rem;\r\n  }\r\n}\r\n.list-box {\r\n  padding: 0.2rem;\r\n}\r\n.colon {\r\n  display: inline-block;\r\n  margin: 0 4px;\r\n  color: #ee0a24;\r\n}\r\n.block {\r\n  display: inline-block;\r\n  width: 22px;\r\n  color: #fff;\r\n  font-size: 12px;\r\n  text-align: center;\r\n  background-color: #ee0a24;\r\n}\r\n.van-button--danger {\r\n  height: 100%;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}