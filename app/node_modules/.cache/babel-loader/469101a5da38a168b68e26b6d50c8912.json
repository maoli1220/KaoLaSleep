    } else {\r\n        try {\r\n          let arr = JSON.parse(localStorage.getItem(\"clock\"));\r\n          arr.unshift({ clock: value.getTime(), status: true });\r\n        } catch {}\r\n        localStorage.setItem(\"clock\", JSON.stringify(arr));\r\n      }\r\n    },\r\n    formatter(type, value) {\r\n      if (type === \"year\") {\r\n        return `${value}年`;\r\n      } else if (type === \"month\") {\r\n        return `${value}月`;\r\n      } else if (type === \"day\") {\r\n        return `${value}日`;\r\n      } else if (type === \"hour\") {\r\n        return `${value}时`;\r\n      } else if (type === \"minute\") {\r\n        return `${value}分`;\r\n      }\r\n      return value;\r\n    },\r\n  },\r\n  components: {\r\n    Tarbar,\r\n    Useless,\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.content {\r\n  padding: 1.333333rem;\r\n}\r\n.wrapper {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 100%;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}