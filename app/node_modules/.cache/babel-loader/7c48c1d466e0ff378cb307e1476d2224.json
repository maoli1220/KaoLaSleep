IAAA,CAAA,SAAA,CAAA,GAAA,CAAA;AACA,OAHA,MAGA;AACA,YAAA,IAAA;;AACA,YAAA;AACA,UAAA,IAAA,GAAA,IAAA,CAAA,KAAA,CAAA,YAAA,CAAA,OAAA,CAAA,OAAA,CAAA,CAAA;;AACA,UAAA,IAAA,CAAA,OAAA,CAAA;AAAA,YAAA,KAAA,EAAA,KAAA,CAAA,OAAA,EAAA;AAAA,YAAA,MAAA,EAAA;AAAA,WAAA;AACA,SAHA,CAGA,OAAA,GAAA,EAAA;AACA,cAAA,GAAA,GAAA,CAAA;AAAA,YAAA,KAAA,EAAA,KAAA,CAAA,OAAA,EAAA;AAAA,YAAA,MAAA,EAAA;AAAA,WAAA,CAAA;AACA,UAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,GAAA,CAAA;AACA;;AACA,QAAA,YAAA,CAAA,OAAA,CAAA,OAAA,EAAA,IAAA,CAAA,SAAA,CAAA,IAAA,CAAA;AACA;;AACA,WAAA,IAAA,GAAA,KAAA;AACA,WAAA,QAAA;AACA,KA1BA;AA2BA,IAAA,SA3BA,qBA2BA,IA3BA,EA2BA,KA3BA,EA2BA;AACA,UAAA,IAAA,KAAA,MAAA,EAAA;AACA,yBAAA,KAAA;AACA,OAFA,MAEA,IAAA,IAAA,KAAA,OAAA,EAAA;AACA,yBAAA,KAAA;AACA,OAFA,MAEA,IAAA,IAAA,KAAA,KAAA,EAAA;AACA,yBAAA,KAAA;AACA,OAFA,MAEA,IAAA,IAAA,KAAA,MAAA,EAAA;AACA,yBAAA,KAAA;AACA,OAFA,MAEA,IAAA,IAAA,KAAA,QAAA,EAAA;AACA,yBAAA,KAAA;AACA;;AACA,aAAA,KAAA;AACA;AAxCA,GAbA;AAuDA,EAAA,UAAA,EAAA;AACA,IAAA,MAAA,EAAA,MADA;AAEA,IAAA,OAAA,EAAA,OAFA;AAGA,IAAA,SAAA,EAAA;AAHA;AAvDA,CAAA","sourcesContent":["<template>\r\n  <div>\r\n    <Tarbar title=\"闹钟\" :right=\"true\">\r\n      <van-icon name=\"plus\" @click=\"add\" slot=\"right\" />\r\n    </Tarbar>\r\n    <div class=\"content\">\r\n      <Useless v-if=\"list.length===0\"></Useless>\r\n      <div class=\"list-box\">\r\n        <ClockList\r\n          v-for=\"(item,index) in list\"\r\n          :key=\"index\"\r\n          :time=\"item.clock\"\r\n          :status=\"item.status\"\r\n          @update=\"getClock\"\r\n        ></ClockList>\r\n      </div>\r\n    </div>\r\n    <van-overlay :show=\"show\" @click=\"show=false\" z-index=\"2\">\r\n      <div class=\"wrapper\" @click.stop>\r\n        <van-datetime-picker\r\n          v-model=\"currentDate\"\r\n          type=\"datetime\"\r\n          title=\"添加闹钟\"\r\n          :min-date=\"minDate\"\r\n          :max-date=\"maxDate\"\r\n          style=\"width:95%\"\r\n          @confirm=\"timeValue\"\r\n          :formatter=\"formatter\"\r\n          @cancel=\"show=false\"\r\n        />\r\n      </div>\r\n    </van-overlay>\r\n  </div>\r\n</template>\r\n<script>\r\nimport Tarbar from \"../components/Tabbar/Tabar\";\r\nimport Useless from \"../components/Useless/Useless\";\r\nimport ClockList from \"../components/ClockList/ClockList\";\r\nexport default {\r\n  data() {\r\n    return {\r\n      show: false,\r\n      minDate: new Date(1970, 0, 1),\r\n      maxDate: new Date(2050, 10, 1),\r\n      currentDate: new Date(),\r\n      list: [],\r\n    };\r\n  },\r\n  created() {\r\n   this.getClock()\r\n  },\r\n  methods: {\r\n    getClock() {\r\n      if (localStorage.getItem(\"clock\") != null) {\r\n        this.list = JSON.parse(localStorage.getItem(\"clock\"));\r\n      }\r\n    },\r\n    add() {\r\n      this.show = true;\r\n    },\r\n    timeValue(value) {\r\n      if (localStorage.getItem(\"clock\") === null) {\r\n        let arr = [{ clock: value.getTime(), status: true }];\r\n        localStorage.setItem(\"clock\", JSON.stringify(arr));\r\n      } else {\r\n        let arr;\r\n        try {\r\n          arr = JSON.parse(localStorage.getItem(\"clock\"));\r\n          arr.unshift({ clock: value.getTime(), status: true });\r\n        } catch (err) {\r\n          let obj = [{ clock: value.getTime(), status: true }];\r\n          localStorage.setItem(\"clock\", JSON.stringify(obj));\r\n        }\r\n        localStorage.setItem(\"clock\", JSON.stringify(arr));\r\n      }\r\n      this.show=false\r\n      this.getClock()\r\n    },\r\n    formatter(type, value) {\r\n      if (type === \"year\") {\r\n        return `${value}年`;\r\n      } else if (type === \"month\") {\r\n        return `${value}月`;\r\n      } else if (type === \"day\") {\r\n        return `${value}日`;\r\n      } else if (type === \"hour\") {\r\n        return `${value}时`;\r\n      } else if (type === \"minute\") {\r\n        return `${value}分`;\r\n      }\r\n      return value;\r\n    },\r\n  },\r\n  components: {\r\n    Tarbar,\r\n    Useless,\r\n    ClockList,\r\n  },\r\n};\r\n</script>\r\n<style scoped>\r\n.content {\r\n  padding: 1.333333rem 0;\r\n}\r\n.wrapper {\r\n  display: flex;\r\n  align-items: center;\r\n  justify-content: center;\r\n  height: 100%;\r\n}\r\n</style>"],"sourceRoot":"src/views"}]}